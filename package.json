  "devDependencies": {
    "debug": "4.1.1",
    "eslint": "7.3.0",
    "eslint-config-htmlacademy": "0.5.2",
    "jest": "26.4.1",
    "nodemon": "2.0.4",
    "pino-debug": "1.3.0",
    "pino-pretty": "4.1.0",
    "pug-lint": "2.6.0",
